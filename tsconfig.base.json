{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "ES2021"],
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    // "noImplicitReturns": true,
  }
}
